// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Completeness */
    "skipLibCheck": true,
    "baseUrl": ".",
         "paths": {
           "api/*": ["src/api/*"],
           "components/*": ["src/components/*"],
           "screens/*": ["src/screens/*"],
           "navigation/*": ["src/navigation/*"],
           "store/*": ["src/store/*"],
           "config/*": ["src/config/*"]
         }
}
}
